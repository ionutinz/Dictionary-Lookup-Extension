{
    "manifest_version": 2,
  
    "name": "Dexonline inz",
    "description": "O extensie care cauta definitia cuvantului selectat pe dexonline.ro",
    "version": "1.0",
    "background": {
        "scripts": ["functions.js"],
        "persistent": false
    },
  
    "browser_action": {
     "default_icon": "icon.png"
    },
    "icons":{
        "16": "search_16x16.png"
    },
    "permissions": [
     "activeTab",
     "tabs",
     "contextMenus"
     ]
  }